{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/public/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html",
      "status": 200
    }
  ]
}