{
    "version": 2,
    "builds": [
        {
            "src": "index.js"
        },
        {
            "src": "public/**/*"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}